# database
DB_DRIVER_NAME=postgres
DB_DATA_SOURCE='postgres://myuser:mypassword@localhost:5432/mydb?sslmode=disable'
DB_MAX_OPEN_CONNS=5
DB_MAX_IDLE_CONNS=5
DB_CONN_MAX_LIFE_TIME_MS=36000000
DB_MIGRATION_CONN_URL='postgres://myuser:mypassword@localhost:5432/mydb?sslmode=disable'
DB_IS_DEV_MODE=true
# kafka
KAFKA_BROKERS=[localhost:9092]
KAFKA_GROUP_ID=rest-service-group
KAFKA_CLIENT_ID=rest-service
KAFKA_VERSION=2.8.0
KAFKA_RACK_ID=localhost
# worker
INTERVAL=5
BATCH_SIZE=10