enum Role {
  MANAGER
  MEMBER
}

type User {
  userid: ID!
  username: String!
  email: String!
  role: Role!
}

input CreateUserInput {
  username: String!
  email: String!
  password: String!
  role: Role!
}

input LoginInput {
  email: String!
  password: String!
}

type Query {
  fetchUsers: [User!]!
}

type Mutation {
  createUser(input: CreateUserInput!): Boolean!
  login(input: LoginInput!): String!
  logout: Boolean!
}